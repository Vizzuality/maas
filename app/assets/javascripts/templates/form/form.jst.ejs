<div class="fixed"></div>

<% if (showPriceFields) { %>

<section>
<div class="left border">
  <div class="inner">
    <ul class="prices other"></ul>
  </div>
</div>
</section>

<section>
<div class="left border">
  <div class="inner">
    <h3 class="total">Total</h3>
  </div>
</div>
<div class="right">
  <div class="inner">
    <span class="subtotal price">Starting from <span>$<%= total %></span></span>
  </div>
</div>
</section>

<% } %>
