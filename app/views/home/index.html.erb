<% content_for :js do %>

  <script id="slideshow-template" type="text/tmpl">
    <a href="#" class="prev"></a>
    <a href="#" class="next"></a>
    <ul class="pagination"></ul>
    <ul class="slides"></ul>
  </script>

  <script type="text/javascript">
    $(function() {
      var slides = new cdb.ui.common.Slides([ { id: 1, title: "Spanish Electoral results map."}, { id: 2, title: "I'm"}, { id: 3, title: "a"}, { id: 4, title: "very cool"}, { id: 5, title: "slideshow"} ]);
      window.slideshow = new cdb.ui.common.Slideshow({ collection: slides });
      slideshow.render();
      slideshow.start();
    });
  </script>
<% end %>
